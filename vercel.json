{
  "version": 2,
  "regions": ["fra1"],
  "functions": {
    "app/api/upload/route.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "VERCEL_MAX_BODY_SIZE": "52428800"
  },
  "trailingSlash": false,
  "cleanUrls": true,
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "crons": [],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
